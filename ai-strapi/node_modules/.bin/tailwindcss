#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules/tailwindcss/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules/tailwindcss/lib/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules/tailwindcss/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/tailwindcss@2.2.19_autopref_f4afb15641e6234b1319d7a2318af739/node_modules/tailwindcss/lib/cli.js" "$@"
fi
