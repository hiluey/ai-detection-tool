#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules/@strapi/cloud-cli/bin/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules/@strapi/cloud-cli/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules/@strapi/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules/@strapi/cloud-cli/bin/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules/@strapi/cloud-cli/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules/@strapi/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/@strapi+cloud-cli@5.11.2/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@strapi/cloud-cli/bin/index.js" "$@"
else
  exec node  "$basedir/../@strapi/cloud-cli/bin/index.js" "$@"
fi
