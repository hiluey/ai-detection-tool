#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/knex@3.0.1_better-sqlite3@11.3.0_sqlite3@5.1.7/node_modules/knex/bin/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/knex@3.0.1_better-sqlite3@11.3.0_sqlite3@5.1.7/node_modules/knex/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/knex@3.0.1_better-sqlite3@11.3.0_sqlite3@5.1.7/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/knex@3.0.1_better-sqlite3@11.3.0_sqlite3@5.1.7/node_modules/knex/bin/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/knex@3.0.1_better-sqlite3@11.3.0_sqlite3@5.1.7/node_modules/knex/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/knex@3.0.1_better-sqlite3@11.3.0_sqlite3@5.1.7/node_modules:/mnt/c/Users/giova/ai-detection-tool/ai-strapi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
